{
  "name": "angular4-swagger-client-generator",
  "version": "0.1.28",
  "description": "Angular 4/5/6 REST API client generator from Swagger JSON with camel case settings",
  "homepage": "https://github.com/lotjomik/angular4-swagger-client-generator",
  "main": "index.js",
  "bin": {
    "a4apigen": "./bin/a4apigen.js"
  },
  "author": {
    "name": "Mika Lötjönen",
    "email": "mika.lotjonen@tieto.com"
  },
  "bugs": {
    "url": "https://github.com/lotjomik/angular4-swagger-client-generator/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:lotjomik/angular4-swagger-client-generator.git"
  },
  "keywords": [
    "swagger",
    "rest",
    "api",
    "generator",
    "angular",
    "angular2",
    "angular4",
    "angular5",
    "angular6",
    "scaffolding"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "a4apigen": "a4apigen -s swagger.json",
    "build": "copyfiles -f ./src/*.js bin && copyfiles -f ./src/generator.js lib",
    "clean": "rimraf bin lib out"
  },
  "dependencies": {
    "copyfiles": "^2.0.0",
    "js-beautify": "^1.7.5",
    "lodash": "^4.17.10",
    "mkdirp": "^0.5.1",
    "mustache": "^2.3.0",
    "optimist": "^0.6.1",
    "request": "^2.86.0",
    "rimraf": "^2.6.2"
  }
}
